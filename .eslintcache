",["171","172","173","174"],"/Users/billy/workspace/projects/personal/strada/strada/src/pages/News.js",["175"],"import React, {useEffect, useRef} from 'react'\nimport {gsap} from 'gsap'\nimport {motion} from \"framer-motion\";\n\nimport Hero from '../components/Hero'\nimport Header from '../components/Header'\nimport Articles from '../components/Articles'\nimport Footer from '../components/Footer'\n\nimport {ScrollTrigger} from 'gsap/ScrollTrigger';\ngsap.registerPlugin(ScrollTrigger);\n\nfunction News() {\n\n    let titleAnim = useRef(null)\n    let subtitleAnim = useRef(null)\n    let tl = gsap.timeline()\n    const LoadingTransition = { duration: 1, ease: [0.43, 0.13, 0.23, 0.96] };\n\n    useEffect(() => {\n\n        tl.from(subtitleAnim, {\n            y: 15,\n            opacity: 0,\n            delay: 1,\n            duration: .5,\n            ease: \"power3.inOut\"\n            })\n            .from(titleAnim, {\n                y: 15,\n                opacity: 0,\n                duration: .5,\n                ease: \"power3.inOut\"\n            })\n            .from(\".scroll-down\", {\n                y: 15,\n                opacity: 0,\n                duration: .5,\n                ease: \"power3.inOut\"\n            });\n\n    }, [titleAnim, subtitleAnim])\n\n    return(\n        <div>\n            <motion.div \n                initial={{x:0}}\n                animate={{x:'-100%'}}\n                exit={{x:0}}\n                transition={LoadingTransition}\n                className=\"page-trans\">\n            </motion.div>\n            <Header />\n            <Hero>\n                <h6 ref={el => subtitleAnim = el} className=\"sub-title\">Latest News</h6>\n                <h1 ref={el => titleAnim = el} className=\"big-title\">We're not just boring developers <br /> We love writting as well.</h1>\n            </Hero>\n            <Articles />\n            <Footer />\n        </div>\n    )\n    \n}\n\nexport default News","/Users/billy/workspace/projects/personal/strada/strada/src/pages/SingleNews.js",["176","177"],"import React, {useEffect, useRef} from 'react'\nimport {useParams} from 'react-router-dom'\nimport {gsap} from 'gsap'\nimport {motion} from \"framer-motion\";\n\nimport Hero from '../components/Hero'\nimport Header from '../components/Header'\nimport NewsDetails from '../components/NewsDetails'\nimport Footer from '../components/Footer'\n\nimport RecentPosts from '../elements/Widgets/RecentPosts'\nimport Categories from '../elements/Widgets/Categories'\n\nimport ArticlesData from '../data/News'\n\nimport {ScrollTrigger} from 'gsap/ScrollTrigger';\ngsap.registerPlugin(ScrollTrigger);\n\nfunction SingleNews(){\n\n    let titleAnim = useRef(null)\n    let subtitleAnim = useRef(null)\n    let tl = gsap.timeline()\n    const LoadingTransition = { duration: 1, ease: [0.43, 0.13, 0.23, 0.96] };\n\n    useEffect(() => {\n\n        tl.from(subtitleAnim, {\n            y: 15,\n            opacity: 0,\n            delay: 1,\n            duration: .5,\n            ease: \"power3.inOut\"\n            })\n            .from(titleAnim, {\n                y: 15,\n                opacity: 0,\n                duration: .5,\n                ease: \"power3.inOut\"\n            })\n            .from(\".scroll-down\", {\n                y: 15,\n                opacity: 0,\n                duration: .5,\n                ease: \"power3.inOut\"\n            });\n\n    }, [titleAnim, subtitleAnim])\n\n    const {newsId} = useParams()\n    const thisNews = ArticlesData.find(element => element.id == newsId)\n\n    return(\n        <div>\n            <motion.div \n                initial={{x:0}}\n                animate={{x:'-100%'}}\n                exit={{x:0}}\n                transition={LoadingTransition}\n                className=\"page-trans\">\n            </motion.div>\n            <Header />\n            <Hero class='single-news-hero'>\n                <h6 ref={el => subtitleAnim = el} className=\"sub-title\"><span className=\"single-news-date\">{thisNews.date}</span>{thisNews.categories.map((item, i) => <a href=\"/\" key={i}>{item}</a>)}</h6>\n                <h1 ref={el => titleAnim = el} className=\"big-title\">{thisNews.title}</h1>\n            </Hero>\n            <NewsDetails>\n                <div className=\"col-12 col-sm-12 col-md-12 col-lg-12 col-xl-8 single-news-left\">\n\n                    <img src={`assets/images/${thisNews.image}`} alt=\"\" />\n                    <h3 className=\"big-title anim-bot\">{thisNews.headline}</h3>\n                    {thisNews.textOne.map((item, i) => <p key={i}>{item}</p>)}\n                    {thisNews.blockquote.text ? \n                        <blockquote>\n                            <p>\n                                {thisNews.blockquote.text}\n                            </p>\n                            <cite>{thisNews.blockquote.author}</cite>\n                        </blockquote>\n                    : null }\n                    {thisNews.textTwo.map((item, i) => <p key={i}>{item}</p>)}\n                    <p className=\"tags\">\n                        {thisNews.tags.map((item, i) => <span key={i}>{item}</span>)}\n                    </p>\n                    \n                </div>\n\n                <div className=\"col-12 col-sm-8 col-md-8 col-lg-6 col-xl-4 single-news-right sidebar\">\n                    <RecentPosts />\n                    <Categories />\n                </div>\n\n                <div className=\"col-12 col-sm-4 col-md-4 col-lg-6\"></div>\n            </NewsDetails>\n            <Footer />\n        </div>\n    )\n\n}\n\nexport default SingleNews","/Users/billy/workspace/projects/personal/strada/strada/src/pages/Contact.js",["178"],"import React, {useEffect, useRef} from 'react'\nimport {gsap} from 'gsap'\nimport {motion} from \"framer-motion\";\n\nimport Hero from '../components/Hero'\nimport Header from '../components/Header'\nimport ContactBody from '../components/ContactBody'\n\nimport {Scr